{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
      { "src": "/pl", "dest": "/src/planet.html" },
     { "src": "/tree", "dest": "/api/tree.html" },
    { "src": "/tr", "dest": "/tr.html" },
    { "src": "/(.*)", "dest": "/api/index.php" }

  ],
  "build": {
    "env": {
      "NOW_PHP_DEBUG": "1"
    }
  }
}
